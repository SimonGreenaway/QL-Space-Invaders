10 OPEN #8,con_512x256a0x0_0
20 INK #8,4
30 CLS #8
40 PRINT #8;"Loading space invaders..."
45 LET v$=VER$
50 IF v$="JSL1" THEN
55 PRINT #8;"JSL1"
60 EXEC_W mdv1_invaders,#8;"-d mdv_ -rom "&VER$(0)&" -sys "&VER$(-2)&" -qdos "&VER$(1)&" -job "&VER$(-1)
70 ELSE
80 EXEC_W mdv1_invaders,#8;"-d mdv_ -rom "&v$
90 END IF
120 CLOSE #8
